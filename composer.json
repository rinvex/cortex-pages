{
    "name": "cortex/pages",
    "description": "Cortex Pages is a frontend layer for an integral part of your Laravel content management system (CMS), it affords an easy, yet powerful way to create and manage pages with full control over their URLs, active status, titles, content, and other attributes.",
    "type": "cortex-module",
    "keywords": [
        "ajax",
        "async",
        "laravel",
        "rinvex",
        "pages",
        "cache",
        "page",
        "cms"
    ],
    "license": "MIT",
    "homepage": "https://rinvex.com",
    "support": {
        "email": "help@rinvex.com",
        "issues": "https://github.com/rinvex/cortex-pages/issues",
        "source": "https://github.com/rinvex/cortex-pages",
        "docs": "https://github.com/rinvex/cortex-pages/README.md"
    },
    "authors": [
        {
            "name": "Rinvex LLC",
            "homepage": "https://rinvex.com",
            "email": "help@rinvex.com"
        },
        {
            "name": "Abdelrahman Omran",
            "homepage": "https://omranic.com",
            "email": "me@omranic.com",
            "role": "Project Lead"
        },
        {
            "name": "The Generous Laravel Community",
            "homepage": "https://github.com/rinvex/cortex-pages/contributors"
        }
    ],
    "require": {
        "php": "^8.0.0",
        "illuminate/auth": "^8.0.0 || ^9.0.0",
        "illuminate/console": "^8.0.0 || ^9.0.0",
        "illuminate/http": "^8.0.0 || ^9.0.0",
        "illuminate/routing": "^8.0.0 || ^9.0.0",
        "illuminate/support": "^8.0.0 || ^9.0.0",
        "cortex/foundation": "^7.0.0",
        "diglactic/laravel-breadcrumbs": "^7.0.0",
        "laravelcollective/html": "^6.2.0",
        "league/fractal": "^0.19.0",
        "proengsoft/laravel-jsvalidation": "^4.4.0",
        "rinvex/laravel-composer": "^6.0.0",
        "rinvex/laravel-menus": "^6.0.0",
        "rinvex/laravel-pages": "^6.0.0",
        "rinvex/laravel-support": "^6.0.0",
        "rinvex/laravel-tenants": "^7.0.0",
        "spatie/laravel-activitylog": "^4.0.0",
        "spatie/laravel-medialibrary": "^9.4.0",
        "yajra/laravel-datatables-buttons": "^4.10.0",
        "yajra/laravel-datatables-fractal": "^1.6.0",
        "yajra/laravel-datatables-html": "^4.36.0",
        "yajra/laravel-datatables-oracle": "^9.15.0"
    },
    "require-dev": {
        "codedungeon/phpunit-result-printer": "^0.31.0",
        "phpunit/phpunit": "^9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Cortex\\Pages\\": "src/",
            "Cortex\\Pages\\Database\\Factories\\": "database/factories/",
            "Cortex\\Pages\\Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Cortex\\Pages\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist",
        "optimize-autoloader": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cortex\\Pages\\Providers\\PagesServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
